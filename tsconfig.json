{
    "compilerOptions": {
        "outDir": "./dist",
        "module": "CommonJS",  
        "target": "ES6",
        "declaration": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src"],
    "exclude": ["dist", "node_modules"]
}
